<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.campustradingplatform">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".PublishOk"></activity>
        <activity
            android:name=".Login.PhoneNumLogin"
            android:launchMode="singleTask" />
        <activity
            android:name=".Login.Register2"
            android:launchMode="singleTask" />
        <activity
            android:name=".Login.Register1"
            android:launchMode="singleTask" />
        <activity
            android:name=".Login.Login"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MsgListActivity" />
        <activity android:name=".MyCenterActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".PublishActivity"></activity>
        <activity android:name=".PublishOk"
            tools:ignore="DuplicateActivity"></activity>
    </application>

</manifest>